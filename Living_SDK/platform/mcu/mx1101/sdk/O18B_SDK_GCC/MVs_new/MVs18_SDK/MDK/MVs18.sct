LR_FLASH 0x00000000 0x800000 - 255 * 1024 {;load region size_region
	ER_INIT 0x00000000  {;execution address = load address
		*.o(EXCEPT_VECTS,+First)
		startup.o
		*.o(InRoot$$Sections)
		*.o(CACHE.4KBMEM)
		*.o(.mach.init)
	}
	ER_MACH0 + 0 {*.o (.mach0.init) }
	ER_MACH1 + 0 {*.o (.mach1.init) }
	;ER_MACH2 + 0 {*.o (.mach2.init) }
	ER_MACH3 + 0 {*.o (.mach3.init) }
	ER_MACH4 + 0 {*.o (.mach4.init) }
	ER_MACH5 + 0 {*.o (.mach5.init) }
