	;ER_MACH7 + 0 {*.o (.mach7.init) }
	ER_MACH8 + 0 {*.o (.mach8.init) }
	ER_MACH9 + 0 {*.o (.mach9.init) }	
	
	
	ER_NOTIFY0 + 0 {*.o (.notify0.init) }
	ER_NOTIFY1 + 0 {*.o (.notify*.init) }
	ER_NOTIFY4 + 0 {*.o (.notify4.init) }
	

	ER_SHELL0 + 0 {*.o (.shell0.cmd) }
	ER_SHELL1 + 0 {*.o (.shell1.cmd) }
	ER_SHELL2 + 0 {*.o (.shell2.cmd) }
	
	ER_ROM_ANY + 0 {
		.ANY (+RO)
	}

	;do not change the execution region name named RW_XMEM
	;you can enlarge or shrink the region size as you want
	RW_XMEM 0x20000000 UNINIT 0x10000 {;64KB XMEM
		*.o (MSP,+FIRST)
		.ANY(+RW,+ZI)
	}

	;if you want to change the RW_XMEM room,change this 0x20010000 
	;for RW_XMEM end boundary only
	RW_XMEM_END 0x20010000 - 4 UNINIT 4{
		*.o (PSP)
	}

